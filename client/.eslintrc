{
  "extends": "react-app",
  "rules": {
    "indent": [
        "error",
        2
    ],
    "linebreak-style": [
        "error",
        "unix"
    ],
    "quotes": [
        "error",
        "double"
    ],
    "semi": [
        "error",
        "always"
    ],
    "react/jsx-uses-vars": [2],
    "eslint linebreak-style": [0, "error", "unix"]
  }
}
